##Set TP type
scoreboard players set #Store lem.battle.tp 1

##Set Chest type
scoreboard players set #Store lem.battle.setchest 1

##Set hunger type
scoreboard players set #Store lem.battle.hungertype 1

##Set glowing setting
scoreboard players set #Store lem.glow 1

##Enable item sets
#Normal
scoreboard players set #Normal lem.battle.enableset 1
#No Armor
scoreboard players set #NoArmor lem.battle.enableset 1
#High Power
scoreboard players set #HighPower lem.battle.enableset 1
#Decayed
scoreboard players set #Decayed lem.battle.enableset 1
#Food Central
scoreboard players set #FoodCentral lem.battle.enableset 1
#Remastered
scoreboard players set #Remastered lem.battle.enableset 1

##Default Map Type
scoreboard players set #Store lem.battle.setmaptype 1

##Set item set count
scoreboard players set #Store lem.battle.setcount 6

##Set take everything
scoreboard players set #Store lem.battle.takeall 1
takeeverything enabled false

##Set healtimer
scoreboard players set #Store lem.battle.healtimer 37

##Set natral regen
scoreboard players set #Store lem.battle.naturalregen 1

##Set Armor Item Swap Deletion bugfix
scoreboard players set #Store lem.battle.armorswapdrop 1
takeeverything deleteItemNotDrop false

##Chest Refill
scoreboard players set #Store lem.battle.chestrefill 1

##Food Central diamond sword fix
scoreboard players set #Store lem.battle.foodcentfix 1

##No Armor leaping potion fix
scoreboard players set #Store lem.battle.leapfix 1

##Large+ Cove Chests fix
scoreboard players set #Store lem.battle.lpluscovefix 1

##Combat style settings
#Combat style
scoreboard players set #Store lem.battle.combatstyle 1
#Attack Cooldown
scoreboard players set #Store lem.battle.attackcooldown 0
scoreboard players set #Store lem.battle.setattackcooldown 0
#Damage values
